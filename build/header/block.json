{
  "$schema": "http://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gsp/header",
  "title": "Site Header",
  "textdomain": "writteninfo",
  "editorScript": "file:./index.js",
  "script": "file:./functions/script.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "supports": {
    "spacing": {
      "padding": true,
      "margin": true
    },
    "typography": {
      "textAlign": true
    }
  },
  "attributes": {
    "is_desktop": {
      "type": "boolean",
      "default": true
    },
    "header_bg": {
      "type": "string",
      "default": "green"
    },
    "title_text": {
      "type": "string",
      "default": "black"
    },
    "item_bg_color": {
      "type": "string",
      "default": "black"
    },
    "item_bg_hover_color": {
      "type": "string",
      "default": "white"
    },
    "item_text_color": {
      "type": "string",
      "default": "white"
    },
    "item_text_hover_color": {
      "type": "string",
      "default": "black"
    },
    "submenu_bg": {
      "type": "string",
      "default": ""
    },
    "child_item_bg_color": {
      "type": "string",
      "default": "black"
    },
    "child_item_bg_hover_color": {
      "type": "string",
      "default": "white"
    },
    "child_item_text_color": {
      "type": "string",
      "default": "white"
    },
    "child_item_text_hover_color": {
      "type": "string",
      "default": "black"
    },
    "item_margin_t": {
      "type": "number",
      "default": 0
    },
    "item_margin_l": {
      "type": "number",
      "default": 5
    },
    "item_margin_r": {
      "type": "number",
      "default": 5
    },
    "item_margin_b": {
      "type": "number",
      "default": 0
    },
    "item_padding_t": {
      "type": "number",
      "default": 0
    },
    "item_padding_l": {
      "type": "number",
      "default": 8
    },
    "item_padding_r": {
      "type": "number",
      "default": 8
    },
    "item_padding_b": {
      "type": "number",
      "default": 0
    },
    "sub_menu_margin_t": {
      "type": "number",
      "default": 10
    },
    "child_item_margin_b": {
      "type": "number",
      "default": 8
    },
    "sidebar_bg": {
      "type": "string",
      "default": "green"
    },
    "sidebar_item_bg_color": {
      "type": "string",
      "default": "black"
    },
    "sidebar_item_bg_hover_color": {
      "type": "string",
      "default": "white"
    },
    "sidebar_item_text_color": {
      "type": "string",
      "default": "white"
    },
    "sidebar_item_text_hover_color": {
      "type": "string",
      "default": "black"
    },
    "sidebar_item_margin_t": {
      "type": "number",
      "default": 0
    },
    "sidebar_item_margin_l": {
      "type": "number",
      "default": 0
    },
    "sidebar_item_margin_r": {
      "type": "number",
      "default": 0
    },
    "sidebar_item_margin_b": {
      "type": "number",
      "default": 10
    },
    "sidebar_item_padding_t": {
      "type": "number",
      "default": 8
    },
    "sidebar_item_padding_l": {
      "type": "number",
      "default": 8
    },
    "sidebar_item_padding_r": {
      "type": "number",
      "default": 8
    },
    "sidebar_item_padding_b": {
      "type": "number",
      "default": 0
    }
  }
}